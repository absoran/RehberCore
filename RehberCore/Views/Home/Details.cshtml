@model List<RehberCore.Models.Number>

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Details</h1>

<div>
    <h4>Number</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model[0].PhoneNumber)
        </dt>
        <dd class="col-sm-10">
            @for (int i = 0; i < Model.Count; i++)
            {
                @Html.DisplayFor(model => model[i].PhoneNumber)<br />
            }
        </dd>
    </dl>
</div>
<div>    
    <a asp-action="ShowAll2">Back to List</a>
</div>
